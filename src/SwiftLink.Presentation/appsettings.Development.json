{
  "AppSettings": {
    "DefaultUrlOnNotFound": "https://agah.com/",
    "DefaultExpirationTimeInDays": "7",
    "DefaultPrometheusPort": 5678,
    "DefaultHealthCheckPort": 9876,
    "Redis": {
      "RedisCacheUrl": "127.0.0.1:6379",
      "SlidingExpirationHour": 5
    },
    "LoggingBehavior": "enable",
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/logs/log-.txt",
          "rolingInterval": "Day",
          "roleOnFileSizeLimit": true,
          "formater": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WriteMachineName",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*"
}
